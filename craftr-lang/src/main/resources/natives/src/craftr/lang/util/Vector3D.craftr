public class Vector3D {
    public double x, y, z;
    //Maybe a "polar" flag?
    
    public Vector3D() {
        this(0,0,0);
    }
    
    public Vector3D(double x, double y, double z) {
        this.x = x;
        this.y = y;
        this.z = z;
    }
}
