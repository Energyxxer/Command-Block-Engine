public class Rotation {
    public float yaw, pitch, roll;
    
    public Rotation() {
        this(0,0,0);
    }
    
    public Rotation(float yaw, float pitch, float roll) {
        this.yaw = yaw;
        this.pitch = pitch;
        this.roll = roll;
    }
}
